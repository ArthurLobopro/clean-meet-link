{
    "manifest_version": 2,
    "version": "0.0.2",
    "name": "Clean Link (meet)",
    "author": "Arthur Lobo",
    "description": "Tira atributos de pesquisa para reuni√µes do google meet",
    "content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["main.js"]
        }
    ],
    "icons": {
        "32": "./assets/icon32.png",
        "48": "./assets/icon48.png"
    },
    "browser_action": {
        "default_popup":"./popup.html"
    },
    "permissions": [
        "unlimitedStorage",
        "storage",
        "activeTab"    
    ]
}